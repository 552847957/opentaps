  <step>
    <name>Sales Team</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>operational</connection>
    <sql>SELECT pty.party_id,
  coalesce(ptygrp.group_name, &apos;Unknown&apos;) as group_name
FROM ((PARTY pty INNER JOIN PARTY_ROLE pr ON pty.party_id = pr.party_id)
LEFT OUTER JOIN PERSON ON pty.party_id = PERSON.party_id)
LEFT OUTER JOIN PARTY_GROUP ptygrp ON pty.party_id = ptygrp.party_id
WHERE pr.role_type_id = &apos;ACCOUNT_TEAM&apos;
ORDER BY ptygrp.group_name ASC;
</sql>
