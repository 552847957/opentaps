  <step>
    <name>Address</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>operational</connection>
    <sql>SELECT 
  pcm.party_id AS addr_party_id,
  a.attn_name,
  a.address1,
  a.city,
  g_country.geo_name AS country,
  g_state.geo_name AS state,
  g_county.geo_name AS region,
  a.postal_code
FROM PARTY_CONTACT_MECH pcm
JOIN POSTAL_ADDRESS a ON a.contact_mech_id = pcm.contact_mech_id 
JOIN PARTY_CONTACT_MECH_PURPOSE pcmp ON pcmp.contact_mech_id = pcm.contact_mech_id 
LEFT JOIN GEO g_country ON g_country.geo_id = a.country_geo_id
LEFT JOIN GEO g_county ON g_county.geo_id = a.county_geo_id
LEFT JOIN GEO g_state ON g_state.geo_id = a.state_province_geo_id
WHERE
  pcm.party_id = ? AND 
  pcmp.contact_mech_purpose_type_id = &apos;GENERAL_LOCATION&apos; AND
  (pcm.thru_date IS NULL OR pcm.thru_date &gt; now()) AND 
  (pcm.from_date &lt; now())
ORDER BY
  pcm.from_date DESC</sql>
    <limit>1</limit>
    <lookup>Customers (Party ID)</lookup>
