    <sql>SELECT
  p.party_id,
  pt.description AS customer_type,
  pe.first_name,
  pe.last_name,
  pg.group_name AS company_name,
  p.created_stamp AS created_date_and_time,
  psd.industry_enum_id
FROM
  PARTY p
