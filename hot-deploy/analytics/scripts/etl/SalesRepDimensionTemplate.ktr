    <connection>operational</connection>
    <sql>SELECT 
  p.party_id AS sales_rep_id,
  p.first_name,
  p.last_name
FROM PARTY_ROLE pr
JOIN PERSON p ON p.party_id = pr.party_id
WHERE pr.role_type_id = &apos;COMMISSION_AGENT&apos;
</sql>
