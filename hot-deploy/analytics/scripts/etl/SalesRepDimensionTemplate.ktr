  <step>
    <name>Add sales rep team name</name>
    <type>DBJoin</type>
    <description/>
    <distribute>N</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>operational</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT
  pg.party_id AS sales_team_id,
  pg.group_name AS sales_team_name
FROM 
  PARTY_GROUP pg
JOIN PARTY_RELATIONSHIP pr ON pg.party_id = pr.party_id_from
WHERE 
  pr.party_id_to = ? AND 
  pr.role_type_id_from = &apos;ACCOUNT_TEAM&apos;
ORDER BY 
  COALESCE(pr.thru_date, &apos;9999-12-31&apos;) DESC, 
  pr.from_date DESC</sql>
    <outer_join>Y</outer_join>
    <parameter>
      <field>
        <name>sales_rep_id</name>
        <type>-</type>
      </field>
    </parameter>
