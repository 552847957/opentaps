    <sql>SELECT
     OH.order_id AS order_id,
     OH.bill_from_party_id AS organization_id,
     OI.order_item_seq_id AS order_item_seq_id,
     OH.sales_channel_enum_id AS sales_channel_enum_id,
     OH.bill_to_party_id AS bill_to_party_id,
     OH.product_store_id AS product_store_id,
     OH.order_date AS order_date,
     OI.product_id AS product_id,
     (OI.quantity - coalesce(cancel_quantity,0)) AS quantity,
     OH.status_id AS order_status_id,
     OI.status_id AS item_status_id,
     OI.unit_price AS unit_price,
     OH.created_by AS created_by
FROM
     ORDER_HEADER OH LEFT OUTER JOIN ORDER_ITEM OI ON OH.order_id = OI.order_id
WHERE
     OH.order_type_id = &apos;SALES_ORDER&apos; AND
     OH.bill_from_party_id LIKE &apos;${organizationId}&apos;</sql>
