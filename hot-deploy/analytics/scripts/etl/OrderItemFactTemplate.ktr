    <connection>operational</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT * FROM (SELECT
     sum(OA.amount) AS discount_amount_base_currency
FROM
     ORDER_ADJUSTMENT OA
WHERE
    OA.order_id = ? AND OA.order_item_seq_id = ?
AND OA.order_adjustment_type_id &lt;&gt; &apos;SALES_TAX&apos;) DISCNT</sql>
    <outer_join>Y</outer_join>
    <parameter>
      <field>
        <name>order_id</name>
        <type>String</type>
