        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>
var status = order_status_id.getString();

var rs = null;

if (status == &quot;ORDER_REJECTED&quot;) {
  rs = &quot;REJECTED&quot;;
} else if (status == &quot;ORDER_CANCELLED&quot;) {
  rs = &quot;CANCELLED&quot;;
} else if (status == &quot;ORDER_COMPLETED&quot;|| 
           status == &quot;ORDER_UNDELIVERABLE&quot;) {
  rs = &quot;COMPLETED&quot;;
} else if (
    status == &quot;ORDER_CREATED&quot;    || 
    status == &quot;ORDER_PROCESSING&quot; || 
    status == &quot;ORDER_APPROVED&quot;   || 
    status == &quot;ORDER_SENT&quot;       || 
    status == &quot;ORDER_HOLD&quot;       ) 
{
  rs = &quot;PENDING&quot;;
}
</jsScript_script>
