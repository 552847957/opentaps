var s = status_id.getString();

var rs = null;

if (s == null) {
  rs = &quot;REJECTED&quot;;
} else if (s == &quot;RETURN_CANCELLED&quot;) {
  rs = &quot;CANCELLED&quot;;
} else if (s == &quot;RETURN_COMPLETED&quot;) {
  rs = &quot;COMPLETED&quot;;
} else {
  rs = &quot;PENDING&quot;;
}
