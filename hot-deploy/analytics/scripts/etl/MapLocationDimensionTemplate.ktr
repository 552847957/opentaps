    <sql>SELECT DISTINCT
  cd.city as city,
  cd.state as state,
  cd.region as region,
  cd.country as country,
  mlp.latitude as cur_latitude,
  mlp.longitude as cur_longitude
FROM CUSTOMER_DIMENSION as cd
LEFT JOIN MAP_LOCATION_DIMENSION as mlp
ON (cd.city like mlp.city
AND cd.state like mlp.state
AND cd.region like mlp.region
AND cd.country like mlp.country)
WHERE cd.region is not null
AND cd.state is not null
AND cd.city is not null
