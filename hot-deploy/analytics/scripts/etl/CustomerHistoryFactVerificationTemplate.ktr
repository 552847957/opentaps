    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#47;&#47;var expected_lifetime_net_order_value = expected_lifetime_order_total - expected_lifetime_return_total;

var delta_lifetime_number_of_orders;
var delta_lifetime_number_of_returns;
delta_lifetime_number_of_orders = delta(expected_lifetime_number_of_orders.getInteger(),  lifetime_number_of_orders.getInteger());
delta_lifetime_number_of_returns = delta(expected_lifetime_number_of_returns.getInteger(), lifetime_number_of_returns.getInteger());

var delta_lifetime_order_total;
var delta_lifetime_return_total;
delta_lifetime_order_total = delta(expected_lifetime_order_total.getNumber(),  lifetime_order_total.getNumber());
delta_lifetime_return_total = delta(expected_lifetime_return_total.getNumber(), lifetime_return_total.getNumber());

var delta_lifetime_net_order_value;
delta_lifetime_net_order_value = delta(expected_lifetime_net_order_value.getNumber(), lifetime_net_order_value.getNumber());

var max_abs_delta = max(abs(delta_lifetime_number_of_orders), abs(delta_lifetime_number_of_orders));
max_abs_delta = max(max_abs_delta, abs(delta_lifetime_order_total));
max_abs_delta = max(max_abs_delta, abs(delta_lifetime_return_total));
max_abs_delta = max(max_abs_delta, abs(delta_lifetime_net_order_value));


&#47;&#47; calculate the delate
function delta(expected, actual) {
  if (actual == null) actual = 0;
  if (expected == null) expected = 0;

  if (actual == expected) return 0;

  if (expected == 0) return -1;
  if (actual == 0) {
    if (expected &gt; 0) return 1;
    return -1;
  }
  
  return (expected - actual)&#47;actual;
}

function max(a, b) {
  if (a &gt; b) return a;
  return b;
}
</jsScript_script>
