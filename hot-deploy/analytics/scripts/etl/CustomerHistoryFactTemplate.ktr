  <step>
    <name>Lifetime Order</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>analytics</connection>
    <sql>SELECT
     OIF.customer_dim_key,
     OIF.organization_id,
     ? as as_of_date_key,
     count(DISTINCT(OIF.order_id)) as lifetime_number_of_orders,
     sum(OIF.net_amount_base_currency) as lifetime_order_total,
     sum(OIF.net_amount_base_currency - coalesce(OIF.returned_net_amount_base_currency, 0)) as lifetime_net_order_value,
     max(OIF.order_date_key) as date_last_order
FROM
     ORDER_ITEM_FACT as OIF
WHERE
     OIF.resolution_status != &apos;CANCELLED&apos;
AND
     OIF.organization_id LIKE &apos;${organizationId}&apos;
AND
     OIF.order_date_key &lt;= ?
AND
     OIF.customer_dim_key IS NOT NULL
GROUP BY
     OIF.customer_dim_key</sql>
    <limit>0</limit>
    <lookup>Generate As Of Date</lookup>
    <execute_each_row>Y</execute_each_row>
