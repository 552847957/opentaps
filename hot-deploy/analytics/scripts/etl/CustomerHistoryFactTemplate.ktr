  <step>
    <name>Generate 12 month back Date</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>analytics</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT
    DD.year as y_now,
    DD.month as m_now,
    DD.day as d_now,
    DD12.year as y_12_month,
    DD12.date_dim_key as as_of_12_month_date_key
FROM
    DATE_DIMENSION as DD,
    DATE_DIMENSION as DD12
WHERE
    DD.year - 1 = DD12.year
AND
    DD.month = DD12.month
AND
    DD.day = DD12.day
AND
    DD.date_dim_key = ?
</sql>
    <outer_join>N</outer_join>
    <parameter>
      <field>
        <name>as_of_date_key</name>
        <type>Integer</type>
      </field>
    </parameter>
