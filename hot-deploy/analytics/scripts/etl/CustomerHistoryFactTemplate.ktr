  <step>
    <name>12 Month Orders</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>analytics</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT
     count(OIF.order_id) as number_of_orders_last_12_month,
     sum(OIF.net_amount_base_currency) as total_orders_last_12_month
FROM
     ORDER_ITEM_FACT as OIF
WHERE
     OIF.resolution_status != &apos;CANCELLED&apos;
AND
     OIF.organization_id LIKE ?
AND
     OIF.customer_dim_key = ?
AND
     OIF.order_date_key &gt; ?
AND
     OIF.order_date_key &lt;= ?
</sql>
    <outer_join>Y</outer_join>
    <parameter>
      <field>
        <name>organization_id</name>
        <type>String</type>
      </field>
      <field>
        <name>customer_dim_key</name>
        <type>Integer</type>
      </field>
      <field>
        <name>as_of_12_month_date_key</name>
        <type>Integer</type>
      </field>
      <field>
        <name>as_of_date_key</name>
        <type>Integer</type>
      </field>
    </parameter>
