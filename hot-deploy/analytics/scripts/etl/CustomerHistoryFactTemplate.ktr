  <step>
    <name>Generate As Of Date</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>analytics</connection>
    <sql>SELECT
    max(DD.date_dim_key) as as_of_date_key,
    max(DD.date_dim_key) as as_of_date_key_limit
FROM
    DATE_DIMENSION as DD
GROUP BY
    DD.year, DD.month
HAVING
    max(DD.date_dim_key) &gt;= ?
AND
    max(DD.date_dim_key) &lt;= ?
ORDER BY
    DD.year, DD.month
</sql>
    <limit>0</limit>
    <lookup>Generate limit As Of Date</lookup>
    <execute_each_row>N</execute_each_row>
