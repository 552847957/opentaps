  <step>
    <name>Category Join</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>operational</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT
  coalesce(pcp.category_name, pc.category_name, p.primary_product_category_id, pc.product_category_id, &apos;Unknown&apos;) as category
FROM
  PRODUCT p
LEFT JOIN PRODUCT_CATEGORY_MEMBER pcm ON p.product_id = pcm.product_id AND ((pcm.from_date is null) or (pcm.from_date &lt; now())) AND ((pcm.thru_date is null) or (pcm.thru_date &gt; now()))
LEFT JOIN PRODUCT_CATEGORY pcp ON p.primary_product_category_id = pcp.product_category_id
LEFT JOIN PRODUCT_CATEGORY pc ON pcm.product_category_id = pc.product_category_id

WHERE
p.product_id = ? AND
pc.product_category_type_id = &apos;CATALOG_CATEGORY&apos; 
LIMIT 1 OFFSET 0
</sql>
    <outer_join>Y</outer_join>
    <parameter>
      <field>
        <name>product_id</name>
        <type>String</type>
      </field>
    </parameter>
