  <step>
    <name>Product</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>operational</connection>
    <sql>SELECT
  p.product_id AS product_id,
  coalesce(p.product_type_id, &apos;Unknown&apos;) AS product_type_id,
  coalesce(p.product_name, p.internal_name, p.product_id, &apos;Unknown&apos;) AS product_name,
  coalesce(p.brand_name, &apos;Unknown&apos;) AS brand_name,
  p.weight AS weight,
  p.product_height AS height,
  p.product_width AS width,
  p.product_depth AS depth,
  coalesce(g1.id_value, &apos;Unknown&apos;) AS sku,
  coalesce(g2.id_value, &apos;Unknown&apos;) AS upc
FROM
  PRODUCT p
LEFT JOIN GOOD_IDENTIFICATION g1 ON p.product_id = g1.product_id AND g1.good_identification_type_id = &apos;SKU&apos;
LEFT JOIN GOOD_IDENTIFICATION g2 ON p.product_id = g2.product_id AND g2.good_identification_type_id = &apos;UPCA&apos;
ORDER BY p.product_id 
</sql>
