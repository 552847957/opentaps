  <step>
    <name>Supplier Join </name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>operational</connection>
    <rowlimit>0</rowlimit>
    <sql>SELECT 
  coalesce(sp.party_id, &apos;Unknown&apos;) AS org_supplier_id,
  sp.last_price AS est_base_currency_cost,
  sp.minimum_order_quantity AS minimum_order_quantity,
  coalesce(pg.group_name, &apos;Unknown&apos;) AS supplier_name
FROM SUPPLIER_PRODUCT sp
LEFT JOIN PARTY_GROUP pg ON pg.party_id = sp.party_id
WHERE
  sp.product_id = ?
ORDER BY
  sp.available_from_date DESC
LIMIT 1 OFFSET 0</sql>
    <outer_join>Y</outer_join>
