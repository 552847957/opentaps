    <jsScripts>      <jsScript>        <jsScript_type>0</jsScript_type>
        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>&#47;&#47; Create localized calendar starting at initial date from input
var locale = new java.util.Locale(locale_language.getString(), locale_country.getString());
var calendar = new java.util.GregorianCalendar(locale);
calendar.setTime(initial_date.getDate());

&#47;&#47; Add number of days generated from sequence
calendar.add(calendar.DAY_OF_MONTH, daysequence.getInteger() - 1);

&#47;&#47; calculate the numeric fields of date_dimension
var year = calendar.get(calendar.YEAR);
var month = calendar.get(calendar.MONTH) + 1;
var day = calendar.get(calendar.DAY_OF_MONTH);
var day_of_week = calendar.get(calendar.DAY_OF_WEEK);
var day_of_year = calendar.get(calendar.DAY_OF_YEAR);
var week_of_year = calendar.get(calendar.WEEK_OF_YEAR);

&#47;&#47; grab instance of simple date format and the Date of this row
var formatter = java.text.SimpleDateFormat(&quot;EEEE&quot;, locale);
var date = new java.util.Date(calendar.getTimeInMillis());

&#47;&#47; generate named fields with formatter and Date
formatter.applyPattern(&quot;EEEE&quot;);
var day_name = formatter.format(date);
formatter.applyPattern(&quot;MMMM&quot;);
var month_name = formatter.format(date);</jsScript_script>
