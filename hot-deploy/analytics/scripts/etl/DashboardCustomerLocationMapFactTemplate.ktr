  <step>
    <name>Get customer locations</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>analytics</connection>
    <sql>SELECT cd.organization_id AS o,
  COUNT(cd.customer_dim_key) AS number, 
  &apos;&apos; as city,
  &apos;&apos; AS state,
  (coalesce(cd.country, &apos;Unknown&apos;)) AS country,
  mld.latitude,
  mld.longitude
FROM CUSTOMER_DIMENSION cd,
  MAP_LOCATION_DIMENSION mld
WHERE  cd.customer_dim_key &gt; 1
AND cd.city IS NULL
AND cd.state IS NULL
AND mld.city IS NULL
AND mld.region IS NULL
AND mld.state IS NULL
AND mld.country = cd.country
GROUP BY cd.country, cd.state, cd.city
UNION
SELECT cd.organization_id AS o,
  COUNT(cd.customer_dim_key) AS number, 
  &apos;&apos; AS city,
  cd.state,
  cd.country,
  mld.latitude,
  mld.longitude
FROM CUSTOMER_DIMENSION cd,
  MAP_LOCATION_DIMENSION mld
WHERE cd.customer_dim_key &gt; 1
AND cd.city IS NULL
AND cd.state IS NOT NULL
AND mld.city IS NULL
AND mld.region IS NULL
AND mld.state = cd.state
AND mld.country = cd.country
GROUP BY cd.country, cd.state, cd.city
UNION
SELECT cd.organization_id AS o,
  COUNT(cd.customer_dim_key) AS number, 
  cd.city,
  &apos;&apos; AS state,
  cd.country,
  mld.latitude,
  mld.longitude
FROM CUSTOMER_DIMENSION cd,
  MAP_LOCATION_DIMENSION mld
WHERE cd.customer_dim_key &gt; 1
AND cd.city IS NOT NULL
AND cd.state IS NULL
AND mld.city = cd.city
AND mld.region IS NULL
AND mld.state IS NULL
AND mld.country = cd.country
GROUP BY cd.country, cd.state, cd.city
UNION
SELECT cd.organization_id AS o,
  COUNT(cd.customer_dim_key) AS number, 
  cd.city,
  cd.state,
  cd.country,
  mld.latitude,
  mld.longitude
FROM CUSTOMER_DIMENSION cd,
  MAP_LOCATION_DIMENSION mld
WHERE cd.customer_dim_key &gt; 1
AND cd.city IS NOT NULL
AND cd.state IS NOT NULL
AND mld.city = cd.city
AND mld.region IS NULL
AND mld.state = cd.state
AND mld.country = cd.country
GROUP BY cd.country, cd.state, cd.city</sql>
