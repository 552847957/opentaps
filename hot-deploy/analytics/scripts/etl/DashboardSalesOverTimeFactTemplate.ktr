  <step>
    <name>Get sales over time data</name>
    <type>TableInput</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>
    <connection>analytics</connection>
    <sql>SELECT
  oif.organization_id,
  &quot;All&quot; as CATEGORY,
  TIMESTAMP(CONCAT(dd.year,&quot;-&quot;,dd.month,&quot;-&quot;,dd.day)) as DATE, 
  SUM(oif.net_amount_base_currency - oif.returned_net_amount_base_currency) AS SOLD_VALUE
FROM ORDER_ITEM_FACT oif
INNER JOIN DATE_DIMENSION dd ON dd.date_dim_key = oif.order_date_key
WHERE 
  oif.resolution_status != &apos;CANCELLED&apos;
GROUP BY dd.year, dd.month
ORDER BY dd.year, dd.month</sql>
    <limit>0</limit>
    <lookup/>
    <execute_each_row>N</execute_each_row>
    <variables_active>N</variables_active>
    <lazy_conversion_active>Y</lazy_conversion_active>
