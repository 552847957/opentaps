  <step>
    <name>Get TOTAL_SII_AMOUNT_FOR_REFUND_ALLOC_CALC</name>
    <type>DBJoin</type>
    <description/>
    <distribute>Y</distribute>
    <copies>1</copies>
         <partitioning>
