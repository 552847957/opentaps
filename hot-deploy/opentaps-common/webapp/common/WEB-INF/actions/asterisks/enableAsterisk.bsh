import java.util.*;
import java.sql.Timestamp;
import org.ofbiz.entity.*;
import org.ofbiz.entity.condition.*;
import org.ofbiz.base.util.*;
import org.opentaps.common.util.*;
/**
 * This function check if enable asterisk.
 */
String enabled = UtilProperties.getPropertyValue("asterisk.properties", "asterisk.enabled", "N");
Debug.log("asterisk enabled:" + enabled); 
if(enabled.equals("Y")) {
	globalContext = context.get("globalContext");
	UtilCommon.addGwtScript(globalContext, "commongwt/org.opentaps.gwt.common.asterisk.asterisk");
	Debug.log("enable commongwt/org.opentaps.gwt.common.asterisk.asterisk"); 
}